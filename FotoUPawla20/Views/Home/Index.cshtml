        <div class="SegmentTittle" style="background-image: url(images/Home/textback1.jpeg);">
            <div class="AttachmentText" style="background-image: url(images/Home/fs.png);"></div>
        </div>
        <div class="ShadowDIVMini">

            <script>
                //Zmienne
                var gal1 = 0;
                var ContePrzygotowania = 0;
                var ConteCeremonia = 0;
                var ConteWesele = 0;
                var ContePlener = 0;

                //Skrypty zachowania przycisków
                function Show10() {
                    //Zachowanie wciśniętego przycisku
                    $('#AdvGal1').css('background-color', '#2962FF');
                    $('#AdvGal1').css('font-weight', '400');
                    $('#AdvGal1').disabled = true;

                    //Zachowanie pozostałych przycisków
                    $('#AdvGal2').css('background-color', '#3F51B5');
                    $('#AdvGal2').css('font-weight', '300');
                    $('#AdvGal2').disabled = false;

                    $('#AdvGal3').css('background-color', '#3F51B5');
                    $('#AdvGal3').css('font-weight', '300');
                    $('#AdvGal3').disabled = false;

                    $('#AdvGal4').css('background-color', '#3F51B5');
                    $('#AdvGal4').css('font-weight', '300');
                    $('#AdvGal4').disabled = false;
                }
                function Show11() {
                    //Zachowanie wciśniętego przycisku
                    $('#AdvGal2').css('background-color', '#2962FF');
                    $('#AdvGal2').css('font-weight', '400');
                    $('#AdvGal2').disabled = true;

                    //Zachowanie pozostałych przycisków
                    $('#AdvGal1').css('background-color', '#3F51B5');
                    $('#AdvGal1').css('font-weight', '300');
                    $('#AdvGal1').disabled = false;

                    $('#AdvGal3').css('background-color', '#3F51B5');
                    $('#AdvGal3').css('font-weight', '300');
                    $('#AdvGal3').disabled = false;

                    $('#AdvGal4').css('background-color', '#3F51B5');
                    $('#AdvGal4').css('font-weight', '300');
                    $('#AdvGal4').disabled = false;
                }
                function Show12() {
                    //Zachowanie wciśniętego przycisku
                    $('#AdvGal3').css('background-color', '#2962FF');
                    $('#AdvGal3').css('font-weight', '400');
                    $('#AdvGal3').disabled = true;

                    //Zachowanie pozostałych przycisków
                    $('#AdvGal2').css('background-color', '#3F51B5');
                    $('#AdvGal2').css('font-weight', '300');
                    $('#AdvGal2').disabled = false;

                    $('#AdvGal1').css('background-color', '#3F51B5');
                    $('#AdvGal1').css('font-weight', '300');
                    $('#AdvGal1').disabled = false;

                    $('#AdvGal4').css('background-color', '#3F51B5');
                    $('#AdvGal4').css('font-weight', '300');
                    $('#AdvGal4').disabled = false;
                }
                function Show13() {
                    //Zachowanie wciśniętego przycisku
                    $('#AdvGal4').css('background-color', '#2962FF');
                    $('#AdvGal4').css('font-weight', '400');
                    $('#AdvGal4').disabled = true;

                    //Zachowanie pozostałych przycisków
                    $('#AdvGal2').css('background-color', '#3F51B5');
                    $('#AdvGal2').css('font-weight', '300');
                    $('#AdvGal2').disabled = false;

                    $('#AdvGal3').css('background-color', '#3F51B5');
                    $('#AdvGal3').css('font-weight', '300');
                    $('#AdvGal3').disabled = false;

                    $('#AdvGal1').css('background-color', '#3F51B5');
                    $('#AdvGal1').css('font-weight', '300');
                    $('#AdvGal1').disabled = false;
                }

                //Skrypt do obsługi galerii ceremoni, wesela, pleneru i przygotowań
                var ChoosenOne = 1;

                var UrlsPrzygotowania = new Array(
                    @Html.Raw(Model.SkryptGaleriiPrzygotowania())
                );

                var UrlsCeremonia = new Array(
                    @Html.Raw(Model.SkryptGaleriiCeremonia())
                );

                var UrlsWesele = new Array(
                    @Html.Raw(Model.SkryptGaleriiWesele())
                );

                var UrlsPlener = new Array(
                    @Html.Raw(Model.SkryptGaleriiPlener())
                );

                var PrzygotowaniaPhotosInt = @Html.Raw(Model.LiczbaZdjecWGaleriiPrzygotowania());
                var CeremoniaPhotosInt = @Html.Raw(Model.LiczbaZdjecWGaleriiCeremonia());
                var WeselePhotosInt = @Html.Raw(Model.LiczbaZdjecWGaleriiWesele());
                var PlenerPhotosInt = @Html.Raw(Model.LiczbaZdjecWGaleriiPlener());

                function PrzygotowaniaFunkcja(arg) {
                    if (arg == 'p') {
                        ContePrzygotowania--;

                        if (ContePrzygotowania == -1)
                            ContePrzygotowania = PrzygotowaniaPhotosInt;
                    }

                    else
                        ContePrzygotowania++;

                    if (ContePrzygotowania == (PrzygotowaniaPhotosInt + 1))
                        ContePrzygotowania = 0;

                    $('#Viewer2').css('background-image', 'url(' + UrlsPrzygotowania[ContePrzygotowania] + ')');
                }

                function CeremoniaFunkcja(arg) {
                    if (arg == 'p') {
                        ConteCeremonia--;

                        if (ConteCeremonia == -1)
                            ConteCeremonia = CeremoniaPhotosInt;
                    }

                    else
                        ConteCeremonia++;

                    if (ConteCeremonia == (CeremoniaPhotosInt + 1))
                        ConteCeremonia = 0;

                    $('#Viewer2').css('background-image', 'url(' + UrlsCeremonia[ConteCeremonia] + ')');
                }

                function WeseleFunkcja() {
                    if (arg == 'p') {
                        ConteWesele--;

                        if (ConteWesele == -1)
                            ConteWesele = WeselePhotosInt;
                    }

                    else
                        ConteWesele++;

                    if (ConteWesele == (WeselePhotosInt + 1))
                        ConteWesele = 0;

                    $('#Viewer2').css('background-image', 'url(' + UrlsWesele[ConteWesele] + ')');
                }

                function PlenerFunkcja() {
                    if (arg == 'p') {
                        ContePlener--;

                        if (ContePlener == -1)
                            ContePlener = PlenerPhotosInt;
                    }

                    else
                        ContePlener++;

                    if (ContePlener == (PlenerPhotosInt + 1))
                        ContePlener = 0;

                    $('#Viewer2').css('background-image', 'url(' + UrlsPlener[ContePlener] + ')');
                }

                function WybierzKategorieGalerii(arg) {
                    switch (arg)
                    {
                        case 1:
                            ChoosenOne = 1;
                            Show10();
                            $('#Viewer2').css('background-image', 'url(' + UrlsPrzygotowania[ContePrzygotowania] + ')');
                            break;

                        case 2:
                            ChoosenOne = 2;
                            Show11();
                            $('#Viewer2').css('background-image', 'url(' + UrlsCeremonia[ConteCeremonia] + ')');
                            break;

                        case 3:
                            ChoosenOne = 3;
                            Show12();
                            $('#Viewer2').css('background-image', 'url(' + UrlsWesele[ConteWesele] + ')');
                            break;

                        case 4:
                            ChoosenOne = 4;
                            Show13();
                            $('#Viewer2').css('background-image', 'url(' + UrlsPlener[ContePlener] + ')');
                            break;

                        default:
                            ChoosenOne = 1;
                            Show10();
                            $('#Viewer2').css('background-image', 'url(' + UrlsPrzygotowania[ContePrzygotowania] + ')');
                            break;
                    }
                }

                function WybierzFunkcjeDrugiejGalerii(arg) {
                    switch (ChoosenOne) {
                        case 1:
                            PrzygotowaniaFunkcja(arg);
                            break;

                        case 2:
                            CeremoniaFunkcja(arg);
                            break;

                        case 3:
                            WeseleFunkcja(arg);
                            break;

                        case 4:
                            PlenerFunkcja(arg);
                            break;
                    }
                }
            </script>

            <div class="SHViewer" id="Viewer2">
                <button class="LeftBtn GaleryBtn Mini-invisible" onClick="WybierzFunkcjeDrugiejGalerii('p')"><</button>
                <button class="RightBtn GaleryBtn Mini-invisible" onClick="WybierzFunkcjeDrugiejGalerii('n')">></button>
                <div class="Big-Invisible" style="position: absolute; left: 0; top: 0; width: 100%; height: 100%; background-color: #ffffff; z-index: 100;"></div>

                <!-- KAFELKI DO RESPONSYWNOŚCI MAŁEJ -->
                <div id="KafelkowyKontener1" class="Big-Invisible">
                    <div class="KafelekMetro1" id="PierwszyKafelekMetro1" style="background-image: url(images/Home/metro0.jpeg);">
                        <div class="MetroText">@Html.ActionLink("Przygotowania", "GaleriaMobilna", "Home", new { Galeria = "Przygotowania" }, null)</div>
                    </div>
                    <div class="KafelekMetro1" style="background-image: url(images/Home/metro1.jpeg);">
                        <div class="MetroText">Ceremonia</div>
                    </div>
                    <div class="KafelekMetro1" style="background-image: url(images/Home/metro2.jpeg);">
                        <div class="MetroText">Wesele</div>
                    </div>
                    <div class="KafelekMetro1" style="background-image: url(images/Home/metro3.jpeg);">
                        <div class="MetroText">Plener</div>
                    </div>
                </div>
            </div>
            <button id="AdvGal1" class="GalleryChoiceBtn Mini-invisible" onClick="WybierzKategorieGalerii(1)">Przygotowania</button>
            <button id="AdvGal2" class="GalleryChoiceBtn Mini-invisible" onClick="WybierzKategorieGalerii(2)">Ceremonia</button>
            <button id="AdvGal3" class="GalleryChoiceBtn Mini-invisible" onClick="WybierzKategorieGalerii(3)">Wesele</button>
            <button id="AdvGal4" class="GalleryChoiceBtn Mini-invisible" onClick="WybierzKategorieGalerii(4)">Plener</button>

            <script>
                Show10();
                $('#Viewer2').css('background-image', 'url(' + UrlsPrzygotowania[ContePrzygotowania] + ')');
            </script>

        </div>
    </div>

